# Letta Self-Hosted Configuration
# Copy this file to .env and fill in your actual values

# Letta server URL (self-hosted, defaults to localhost:8283)
LETTA_API_URL=http://localhost:8283

# Model configuration
LETTA_MODEL=xai/grok-3-mini
XAI_API_KEY=your_xai_api_key_here

# Ollama configuration (for free embeddings)
OLLAMA_API_URL=http://localhost:11434
LETTA_OLLAMA_EMBEDDING_MODEL=all-minilm

# Optional: Context window limit for Grok models (defaults to 30000)
# LETTA_CONTEXT_WINDOW_LIMIT=30000
